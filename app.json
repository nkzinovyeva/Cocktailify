{
  "expo": {
    "name": "Cocktailify",
    "slug": "cocktailify",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/cocktail_1",
    "splash": {
      "image": "./assets/cocktail_1",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/cocktail_1",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/cocktail_1"
    },
    "description": "Enjoy cocktails wherever and whenever you want. Choose, explore, add to favorites, save ingredients to your shopping list!. Made by Natalia Zinovyeva as a study project for the *Mobile Programming* course"
  }
}
